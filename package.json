{
  "name": "mp3-file-analysis",
  "version": "1.0.0",
  "description": "MP3 File Analysis Backend - NestJS application for counting MP3 frames",
  "main": "dist/tasks/file-upload/main.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "author": "James Pierce",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "file-upload:start": "node dist/tasks/file-upload/main.js",
    "file-upload:start:dev": "npm run build && nest start --debug --watch --entryFile tasks/file-upload/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{ts,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,json,md}\"",
    "cdk:synth": "cdk synth",
    "cdk:deploy": "cdk deploy",
    "cdk:destroy": "cdk destroy",
    "cdk:diff": "cdk diff"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.8",
    "@nestjs/core": "^10.4.8",
    "@nestjs/platform-express": "^10.4.8",
    "aws-cdk-lib": "^2.232.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.8",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.4.8",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.2",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "aws-cdk": "^2.1034.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.5.0",
    "jest": "^30.2.0",
    "prettier": "^3.7.4",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0"
  }
}
